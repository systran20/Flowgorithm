        <body>
            <comment text="bir ihtimalin ger&#231;ekle&#351;mesini belirli bir ORANA BA&#286;LAMA"/>
            <declare name="Bir, Sifir" type="Integer" array="False" size=""/>
            <declare name="sayi" type="Integer" array="False" size=""/>
            <declare name="&#304;" type="Integer" array="False" size=""/>
