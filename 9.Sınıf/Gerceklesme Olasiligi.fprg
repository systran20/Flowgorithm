                    <else>
                        <comment text="Buras&#305; %30 &#199;ALI&#350;IR"/>
                        <output expression="0" newline="False"/>
                        <assign variable="Sifir" expression="Sifir + 1"/>
                    </else>
                </if>
            </for>
        </body>
    </function>
